package math;

import javax.swing.JOptionPane;

public class atividadeMath5 {

    public static void main(String[] args) {

        int num1 = (int) (Math.random() * 11);

        JOptionPane.showMessageDialog(null, "Numero aleatorio " + num1);

    }
}
